def cadena_parentesis_valida(cadena):
    pila=[]
    # correspondiente :
    parentesis = {'{': '}', '(': ')', '[':']'}

    # interar 
    for c in cadena:
        if c in parentesis:
            # iterando caracter por caracter
            pila.append(c)
        elif len(pila) == 0 or c !=parentesis[pila.pop()]:
            return False
            
    return len(pila)==0

print (cadena_parentesis_valida('()[]{}'))
print (cadena_parentesis_valida('()[]{}]'))
